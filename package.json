{
  "name": "@neabyte/fast-router",
  "version": "0.1.0",
  "description": "A fast, versatile router with radix tree structure for JavaScript",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "unbuild --minify",
    "clean": "rm -rf dist",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "keywords": [
    "api",
    "api-router",
    "deno",
    "express",
    "fast",
    "framework",
    "http",
    "http-router",
    "javascript",
    "lightweight",
    "minimal",
    "middleware",
    "named-parameters",
    "nodejs",
    "optimized",
    "path-matching",
    "performance",
    "radix-tree",
    "regex-validation",
    "request-router",
    "rest-api",
    "route-handler",
    "router",
    "routing",
    "tree",
    "trie",
    "typescript",
    "url-pattern",
    "url-router",
    "web-framework",
    "wildcard-routing"
  ],
  "devDependencies": {
    "unbuild": "^3.6.1"
  },
  "author": {
    "name": "NeaByteLab",
    "email": "me@neabyte.com",
    "url": "https://github.com/NeaByteLab"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeaByteLab/Fast-Router.git"
  },
  "bugs": {
    "url": "https://github.com/NeaByteLab/Fast-Router/issues"
  },
  "homepage": "https://github.com/NeaByteLab/Fast-Router#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
